package com.leyou.data.role {

	public class ProperInfo {
		
		
		public function ProperInfo() {
		}
		
		
		/**
		 *生命值
		 */
		public var hp:int;
		/**
		 *魔法值
		 */
		public var mp:int;
		/**
		 *物理攻击
		 */
		public var phAtt:int;
		/**
		 *物理防禦
		 */
		public var phDef:int;
		/**
		 *魔法攻擊
		 */
		public var magicAtt:int;
		/**
		 *魔法防禦
		 */
		public var magicDef:int;
		/**
		 *暴擊
		 */
		public var crit:int;
		/**
		 *韧性
		 */
		public var tenacity:int;
		/**
		 *命中
		 */
		public var hit:int;
		/**
		 *闪避
		 */
		public var dodge:int;
		/**
		 *必杀
		 */
		public var slay:int;
		/**
		 *守护
		 */
		public var guaid:int;
		
		/**
		 *	精力
		 *	（翅膀装备专用）
		 */
		public var Energy:String;
		
		/**
		 *战力值
		 */
		public var fight:int;
	}
}